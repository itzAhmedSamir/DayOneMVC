@model List<Employee>
@using Day_1.DAL.Entities


<table id="myTable" class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Age</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var data in Model)
        {
            <tr>
                <th scope="row">@data.Id</th>
                <td>@data.Name</td>
                <td>@data.Age</td>
            </tr>
        }
        
    </tbody>
</table>